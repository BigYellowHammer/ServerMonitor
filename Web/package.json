{
  "name": "web",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-scripts": "2.1.1",
    "antd": "^3.2.1",
    "axios": "^0.18.0",
    "dateformat": "^3.0.3",
    "filesize": "^3.6.1",
    "less": "^2.7.3",
    "moment": "^2.20.1",
    "react-redux": "^5.0.6",
    "react-page-visibility": "^2.0.0",
    "react-table": "^6.8.6",
    "redux": "^3.7.2",
    "redux-saga": "^0.16.0",
    "eslint-config-prettier": "^3.1.0",
    "eslint-plugin-prettier": "^3.0.0",
    "node-sass": "^4.10.0",
    "prettier": "^1.15.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app",
    "rules": {
      "global-require": "off",
      "react/jsx-filename-extension": [
        1,
        {
          "extensions": [
            ".js",
            ".jsx"
          ]
        }
      ],
      "linebreak-style": 0,
      "comma-dangle": [
        "error",
        "never"
      ],
      "react/no-unused-prop-types": "warn",
      "no-unused-vars": "warn",
      "no-debugger": 0,
      "no-console": 0,
      "no-param-reassign": [
        "error",
        {
          "props": false
        }
      ]
    }
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
