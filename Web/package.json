{
  "name": "server-monitor",
  "version": "0.1.0",
  "private": true,
  "homepage": "./",
  "dependencies": {
    "@craco/craco": "5.6.4",
    "antd": "3.26.0",
    "babel-plugin-import": "1.13.0",
    "craco-antd": "1.14.1",
    "dateformat": "3.0.3",
    "filesize": "6.0.1",
    "final-form": "4.18.6",
    "final-form-arrays": "3.0.2",
    "ky": "^0.16.1",
    "node-sass": "4.13.1",
    "react": "16.12.0",
    "react-dom": "16.12.0",
    "react-final-form": "6.3.3",
    "react-final-form-arrays": "3.1.1",
    "react-page-visibility": "5.1.0",
    "react-redux": "7.1.3",
    "react-router-dom": "5.1.2",
    "react-scripts": "3.2.0",
    "recharts": "^1.8.3",
    "redux": "4.0.4",
    "redux-saga": "1.1.3"
  },
  "scripts": {
    "start": "craco start",
    "build": "craco build",
    "test": "craco test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "prettier"
    ],
    "plugins": [
      "prettier"
    ],
    "rules": {
      "global-require": "off",
      "react/jsx-filename-extension": [
        1,
        {
          "extensions": [
            ".js",
            ".jsx"
          ]
        }
      ],
      "linebreak-style": 0,
      "comma-dangle": [
        "error",
        "never"
      ],
      "react/no-unused-prop-types": "warn",
      "no-unused-vars": "warn",
      "no-debugger": 0,
      "no-console": 0,
      "no-param-reassign": [
        "error",
        {
          "props": false
        }
      ],
      "prettier/prettier": [
        "error"
      ]
    }
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "eslint": "6.7.2",
    "eslint-config-airbnb": "18.0.1",
    "eslint-config-prettier": "6.7.0",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-prettier": "3.1.1",
    "eslint-plugin-react": "7.17.0",
    "prettier": "1.19.1"
  }
}
