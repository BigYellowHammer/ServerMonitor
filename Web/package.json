{
  "name": "server-monitor",
  "version": "0.1.0",
  "private": true,
  "homepage": "./",
  "dependencies": {
    "@craco/craco": "^5.2.1",
    "antd": "^3.18.2",
    "axios": "^0.18.0",
    "babel-plugin-import": "^1.11.2",
    "craco-antd": "^1.11.0",
    "dateformat": "^3.0.3",
    "eslint-config-prettier": "^4.3.0",
    "eslint-plugin-prettier": "^3.1.0",
    "filesize": "^4.1.2",
    "final-form": "4.8.1",
    "final-form-arrays": "1.1.2",
    "node-sass": "^4.12.0",
    "prettier": "^1.17.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-final-form": "4.1.0",
    "react-final-form-arrays": "2.0.3",
    "react-page-visibility": "^3.2.1",
    "react-redux": "^6.0.1",
    "react-router-dom": "^4.3.1",
    "react-scripts": "3.0.1",
    "react-table": "^6.10.0",
    "redux": "^4.0.1",
    "redux-saga": "^1.0.2",
  },
  "scripts": {
    "start": "craco start",
    "build": "craco build",
    "test": "craco test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app",
    "rules": {
      "global-require": "off",
      "react/jsx-filename-extension": [
        1,
        {
          "extensions": [
            ".js",
            ".jsx"
          ]
        }
      ],
      "linebreak-style": 0,
      "comma-dangle": [
        "error",
        "never"
      ],
      "react/no-unused-prop-types": "warn",
      "no-unused-vars": "warn",
      "no-debugger": 0,
      "no-console": 0,
      "no-param-reassign": [
        "error",
        {
          "props": false
        }
      ]
    }
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}