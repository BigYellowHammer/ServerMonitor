{
  "extends": ["react-app", "plugin:prettier/recommended"],
  "plugins": ["prettier"],
  "rules": {
    "eqeqeq": "error",
    "global-require": "off",
    "react/jsx-filename-extension": [
      1,
      {
        "extensions": [".js", ".jsx"]
      }
    ],
    "arrow-parens": "off",
    "linebreak-style": ["error", "unix"],
    // "linebreak-style": "off",
    "comma-dangle": "off",
    "react/no-unused-prop-types": "warn",
    "no-unused-vars": "warn",
    "no-debugger": "off",
    "no-console": "off",
    "no-param-reassign": [
      "error",
      {
        "props": false
      }
    ],
    "prettier/prettier": ["error"]
  }
}
