{
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": ["airbnb", "react-app", "prettier"],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "plugins": ["react", "prettier"],
  "rules": {
    "endOfLine": "auto",
    "global-require": "off",
    "react/jsx-filename-extension": [
      1,
      {
        "extensions": [".js", ".jsx"]
      }
    ],
    "linebreak-style": 0,
    "comma-dangle": ["error", "never"],
    "react/no-unused-prop-types": "warn",
    "no-unused-vars": "warn",
    "no-debugger": 0,
    "no-console": 0,
    "no-param-reassign": [
      "error",
      {
        "props": false
      }
    ],
    "prettier/prettier": ["error"]
  }
}
